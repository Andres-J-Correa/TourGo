{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Hosting.Internal.WebHost": "Information",
      "Microsoft.AspNetCore.Mvc.Internal.ControllerActionInvoker": "Information"
    },
    "Console": {
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "[HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*"
}
